<h2>Not Required</h2> 
     <% if (!!notRequired) { -%>
        <%  Object.keys(notRequired).forEach(function(key) { -%>      
                <div class="wrap-collabsible">
                <input id="<%= key %>NotRequired" class="toggle" type="checkbox">
                <label for="<%= key %>NotRequired" class="lbl-toggle"><%= key %></label>
                <div class="collapsible-content">
                    <div class="content-inner">
                        <table>
                            <tr>
                                <th>Scenario</th>
                                <th>Required By</th>
                            </tr>
                        <% for (const feature of todo[key])  { -%>
                            <tr>
                                <td><%= feature.scenarioName %></td>
                                <td><%= feature.requiredBy %></td>
                                </tr>
                            <% } -%>
                        </table>
                </div>
                </div>
                </div> 
        <% }); -%>
    <% } -%>